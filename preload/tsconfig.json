{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./",
    "outDir": "../dist/main/preload"
  },
  "display": "Recommended",
  "$schema": "https://json.schemastore.org/tsconfig",
  "include": ["../types/*.d.ts", "./**/*.ts"]
}
